        };

        combo_dot_num {
            bindings = <&kp DOT>;
            key-positions = <13 12>;
            timeout-ms = <100>;
            layers = <9>;
        };

        combo_comma_num {
            bindings = <&kp COMMA>;
            key-positions = <11 12>;
            timeout-ms = <100>;
            layers = <9>;
        };

        combo_dot_rus_num {
            bindings = <&en DOT>;
            key-positions = <12 13>;
            timeout-ms = <100>;
            layers = <12>;
        };

        combo_comma_rus_num {
            bindings = <&en COMMA>;
            key-positions = <11 12>;
            timeout-ms = <100>;
            layers = <12>;
        };

        combo_lt_num {
            bindings = <&kp LT>;
            key-positions = <21 22>;
            timeout-ms = <100>;
            layers = <9>;
        };

        combo_gt_num {
            bindings = <&kp GT>;
            key-positions = <23 22>;
            timeout-ms = <100>;
            layers = <9>;
        };

        combo_lt_rus_num {
            bindings = <&en LT>;
            key-positions = <21 22>;
