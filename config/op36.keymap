        };

        combo_caret_num {
            bindings = <&kp CARET>;
            key-positions = <20 21>;
            timeout-ms = <100>;
            layers = <9>;
        };

        combo_caret_rus_num {
            bindings = <&en CARET>;
            key-positions = <20 21>;
            timeout-ms = <100>;
            layers = <12>;
