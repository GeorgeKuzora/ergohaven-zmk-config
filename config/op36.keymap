        };

        combo_dllr_rus_sym {
            bindings = <&en DLLR>;
            key-positions = <12 13>;
            timeout-ms = <100>;
            layers = <11>;
        };

        combo_pipe_sym {
            bindings = <&kp PIPE>;
            key-positions = <11 12>;
            timeout-ms = <100>;
            layers = <8>;
        };

        combo_pipi_rus_sym {
            bindings = <&en PIPE>;
            key-positions = <11 12>;
            timeout-ms = <100>;
            layers = <11>;
        };

        combo_equal_num {
            bindings = <&kp EQUAL>;
            key-positions = <10 11>;
            timeout-ms = <100>;
            layers = <9>;
        };

        combo_equal_rus_num {
            bindings = <&en EQUAL>;
            key-positions = <10 11>;
            timeout-ms = <100>;
            layers = <12>;
        };

        combo_plus_num {
